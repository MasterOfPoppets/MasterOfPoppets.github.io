{{> header}}

<div class="splash-container">
    <div class="splash">
        <h1 class="splash-head">Quantum</h1>
        <p>mean web development</p>
    </div>
</div>

<div class="main-wrapper">
    <ul>
        {{#each collections.homePosts}}
        <li>
            <h3>
                <a href="{{link this.path}}">{{this.title}}</a>
            </h3>
            <article>{{{this.excerpt}}}</article>
        </li>
        {{/each}}
    </ul>
</div>

{{> footer}}